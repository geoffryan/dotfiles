[user]
	name = Geoffrey Ryan
	email = geoffrey.s.ryan@gmail.com
[core]
	editor = vim
[merge]
    tool = vimdiff
[alias]
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
[push]
	default = current
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[pull]
    rebase = false
